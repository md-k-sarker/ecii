# default namespace
namespace=http://www.daselab.com/residue/analysis#

# K1/negExprTypeLimit, limit of number of concepts in a negative expression of a hornClause
conceptLimitInNegExpr=1

# k10/ maximum negclasses (top scoring) to do the combination. size would be nCr or negClassListMaxSize--C--conceptLimitInNegExpr
negClassListMaxSize=5

# K2/hornClauseLimit
hornClauseLimit=2

# K3/permutate/combination untill this number of objectproperties
objPropsCombinationLimit=1

# K4/posExprTypeLimit, limit of number of concepts in a positive expression of a hornClause
conceptLimitInPosExpr=2

# k9/ maximum posclasses (top scoring) to do the combination. size would be nCr or posClassListMaxSize--C--conceptLimitInPosExpr
posClassListMaxSize=10

# K5 select upto k5 hornClauses to make combination
hornClausesListMaxSize=5

# K6 select upto k6 candidate classes to make combination
candidateClassesListMaxSize=5

# K7/atomic types both appeared in positive and negative
removeCommonTypes=true

# k8, validate solutions upto this number of top solutions
validateByReasonerSize=0

# score type
scoreType=recall

# reasoner
reasoner.reasonerImplementation=pellet

# declare some prefixes to use as abbreviations
prefixes = [ ("ex","http://www.daselab.com/residue/analysis#") ]

# knowledge source definition
ks.fileName = "7_ifp_combined_with_wiki_V3.owl"

# object properties
objectProperties={":talksAbout"}


# learning problem
lp.type = "posNegStandard"
#lp.type = "posonlylp"
lp.positiveExamples = {"ex:IFP_274"}

lp.negativeExamples = { "ex:IFP_1389"}